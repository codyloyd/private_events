<h1>
  <%= @user.name %>
</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-2">
    <h2>events created:</h2>
    <ul class="list">
      <% @events_created.each do |event| %>
      <li>
        <%= link_to event.title, event %>
      </li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-4">
    <h2>upcoming events:</h2>
    <ul class="list">
      <% @upcoming_events.each do |event| %>
      <li>
        <%= link_to event.title, event %>
      </li>
      <% end %>
    </ul>
    <h2>past events:</h2>
    <ul class="list">
      <% @past_events.each do |event| %>
      <li>
        <%= link_to event.title, event %>
      </li>
      <% end %>
    </ul>
  </div>
</div>
